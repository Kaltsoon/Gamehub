<ul class="media-list">
  <% users.each do |user| %>
  <li class="media">
    <a class="pull-left" href="#">
      <%= image_tag user.avatar.url(:thumb), class: "media-object img-rounded" %>
    </a>
    <div class="media-body">
      <h4><%= link_to user, user_path(user) %></h4>
	    <p>
	    	<%= user.synopsis %>
	    </p>
    </div>
  </li>
  <% end %>
</ul>